{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Spring": {
    "Cloud": {
      "Stream": {
        "Bindings": {
          "input1": {
            "Group": "steeltoestream",
            "Destination": "test1"
          },
          "input2": {
            "Destination": "test2"
          },
          "output1": {
            "producer": {
                    "requiredGroups": "steeltoestream"
                  },
              "Destination": "test1"
            },
            "output2": {
              "Destination": "test2"
            }

          }
      }
    }
  }
}