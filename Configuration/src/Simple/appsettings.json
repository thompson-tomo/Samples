{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Spring": {
    "Application": {
      "Name": "foo"
    },
    "Cloud": {
      "Config": {
        "HealthEnabled": true,
        "Discovery": {
          "Enabled": true
        },
        "Uri": "http://somethingwrong:8888",
        "ValidateCertificates": false
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Steeltoe.Extensions.Configuration.ConfigServer": "Trace"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh" ]
        }
      }
    }
  }
  // Discovery-first
  ,"Eureka": {
    "Client": {
      "ServiceUrl": "http://localhost:8761/eureka/",
      "ShouldRegisterWithEureka": false
    }
  }
}
